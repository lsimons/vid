<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Copyright 2014 The VID Authors. Licensed under CC by-nc-nd 4.0. -->
<suite name="vid-functional-tests" verbose="2" parallel="classes">
  <test name="functional">
    <groups>
      <run>
        <include name="checkin"/>
        <include name="functional"/>
        <exclude name="broken"/>
      </run>
    </groups>
    <packages>
      <package name="io.virga.*"/>
    </packages>
  </test>
</suite>
